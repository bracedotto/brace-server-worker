{
  "name": "brace-server-worker",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.x.x"
  },
  "type": "module",
  "scripts": {
    "version": "node --version",
    "diff": "diff const.js ../brace-server/const.js && diff utils.js ../brace-server/utils.js",
    "start": "node --experimental-specifier-resolution=node src/index.js",
    "start-local": "GOOGLE_APPLICATION_CREDENTIALS=~/.config/gcloud/legacy_credentials/brace@brace.to/adc.json node --experimental-specifier-resolution=node src/index.js",
    "gen-image-name": "node --experimental-specifier-resolution=node bin/gen-image-name.js",
    "test-headful": "node --experimental-specifier-resolution=node src/test-headful.js"
  },
  "dependencies": {
    "@google-cloud/datastore": "^7.0.0",
    "@google-cloud/storage": "^6.2.3",
    "puppeteer": "^15.5.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.0",
    "url-parse": "^1.5.10"
  }
}
